{
  "name": "pockito",
  "version": "0.1.6",
  "description": "A lightweight framework for managing app-state in JavaScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js src -d dist",
    "prepublish": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "babel": {
    "presets": [
      "es2015"
